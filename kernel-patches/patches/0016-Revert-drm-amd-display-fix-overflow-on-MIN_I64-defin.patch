 	(int64_t)((1ULL << 63) - 1)
 
-#define MIN_I64 \
-	(-MAX_I64 - 1)
-
 #define FRACTIONAL_PART_MASK \
 	((1ULL << BW_FIXED_BITS_PER_FRACTIONAL_PART) - 1)
 
