-	GEM_BUG_ON(!intel_iov_is_pf(iov));
-
-	for (n = iov->pf.provisioning.num_pushed; n > 0; n--) {
-		err = guc_action_update_vf_cfg(guc, n, 0, 0);
-		if (unlikely(err < 0))
-			break;
-	}
-	iov->pf.provisioning.num_pushed = n;
