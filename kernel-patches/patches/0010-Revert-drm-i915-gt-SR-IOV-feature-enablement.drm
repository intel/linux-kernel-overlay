-static bool pf_continue_as_native(struct drm_i915_private *i915, const char *why)
-{
-#if IS_ENABLED(CONFIG_DRM_I915_DEBUG_GEM)
-	drm_dbg(&i915->drm, "PF: %s, continuing as native\n", why);
-#endif
