-int intel_iov_selftest_send_vfpf_get_ggtt_pte(struct intel_iov *iov, u64 ggtt_addr, u64 *pte)
-{
-	u32 request[VF2PF_ST_GET_GGTT_PTE_REQUEST_MSG_LEN];
