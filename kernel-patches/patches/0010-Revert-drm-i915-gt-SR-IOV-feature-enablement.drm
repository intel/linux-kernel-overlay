-static void pf_assign_ctxs_for_pf(struct intel_iov *iov)
-{
-	struct intel_iov_provisioning *provisioning = &iov->pf.provisioning;
-	u16 total_vfs = pf_get_totalvfs(iov);
-	const u16 total_ctxs_bits = ctxs_bitmap_total_bits();
