-static int guc_update_policy_klv32(struct intel_guc *guc, u16 key, u32 value)
-{
-	const u32 len = 1; /* 32bit value fits into 1 klv dword */
-	const u32 cfg_size = (GUC_KLV_LEN_MIN + len);
-	struct i915_vma *vma;
-	u32 *cfg;
