-static void pf_teardown_tree(struct drm_i915_private *i915)
-{
-	struct i915_sriov_pf *pf = &i915->sriov.pf;
-	struct i915_sriov_ext_kobj **kobjs = fetch_and_zero(&pf->sysfs.kobjs);
-	unsigned int count = pf_nodes_count(i915);
-	unsigned int n;
-
-	if (!kobjs)
-		return;
-
-	for (n = 0; n < count; n++)
