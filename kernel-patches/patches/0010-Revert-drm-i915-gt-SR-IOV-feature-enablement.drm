-		return ret;
-
-	*cfg++ = FIELD_PREP(GUC_KLV_0_KEY, key) | FIELD_PREP(GUC_KLV_0_LEN, len);
-	*cfg++ = value;
-
-	ret = guc_action_update_policy_cfg(guc, intel_guc_ggtt_offset(guc, vma), cfg_size);
-
-	i915_vma_unpin_and_release(&vma, I915_VMA_RELEASE_MAP);
-
-	if (unlikely(ret < 0))
-		return ret;
-	if (unlikely(!ret))
-		return -ENOKEY;
-	if (unlikely(ret > 1))
-		return -EPROTO;
-
-	return 0;
