- * Setup GT IOV provisioning sysfs.
- *
- * Return: 0 on success or a negative error code on failure.
- */
-int intel_iov_sysfs_setup(struct intel_iov *iov)
-{
-	int err;
-
-	if (!intel_iov_is_pf(iov))
-		return 0;
-
-	err = pf_setup_provisioning(iov);
-	if (unlikely(err))
-		goto failed;
-
-	return 0;
-
-failed:
