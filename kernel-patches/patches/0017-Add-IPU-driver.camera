+		.platform_data = &ar0234_pdata_1,
+	},
+	.i2c_adapter_bdf = "0000:00:15.1",
+	},
+	.fixup_spdata = ar0234_fixup_spdata,
+};
+
+static struct ipu_isys_csi2_config ar0234_csi2_cfg_2 = {
+	.nlanes = AR0234_LANES,
+	.port = 2,
+};
+
+static struct ar0234_platform_data ar0234_pdata_2 = {
