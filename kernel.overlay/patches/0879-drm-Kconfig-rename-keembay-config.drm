From cf67b0d4e5c62d6c006f5b77baeda789b84773d0 Mon Sep 17 00:00:00 2001
From: Dave Airlie <airlied@redhat.com>
Date: Fri, 13 Nov 2020 14:53:34 +1000
Subject: [PATCH 0879/1714] drm/Kconfig: rename keembay config

This all caps looked ugly.

Signed-off-by: Dave Airlie <airlied@redhat.com>
---
 drivers/gpu/drm/kmb/Kconfig | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/drivers/gpu/drm/kmb/Kconfig b/drivers/gpu/drm/kmb/Kconfig
index 3921c57ac511..bc4cb5e1cd8a 100644
--- a/drivers/gpu/drm/kmb/Kconfig
+++ b/drivers/gpu/drm/kmb/Kconfig
@@ -1,5 +1,5 @@
 config DRM_KMB_DISPLAY
-	tristate "INTEL KEEMBAY DISPLAY"
+	tristate "Intel Keembay Display"
 	depends on DRM
 	depends on ARCH_KEEMBAY || COMPILE_TEST
 	select DRM_KMS_HELPER
-- 
2.25.1

